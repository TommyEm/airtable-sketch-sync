{
  "name": "AirSketch",
  "description": "Sync content from Airtable to Sketch.",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "suppliesData": true,
  "commands": [
    {
      "script": "sync-layers.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "Shutdown": "onShutdown",
          "SupplyData": "onSupplyData"
        }
      }
    },
    {
      "name": "📝 Sync all artboards",
      "identifier": "airsketch.sync-all-artboards-identifier",
      "script": "sync-artboards.js",
      "handlers": {
        "run": "syncAllArtboards"
      },
      "description": "Sync content with Airtable on all artboards."
    },
    {
      "name": "📝 Sync selected artboards",
      "identifier": "airsketch.sync-selected-artboards-identifier",
      "script": "sync-artboards.js",
      "shortcut": "cmd shift d",
      "handlers": {
        "run": "syncSelectedArtboards"
      },
      "description": "Sync content with Airtable on selected artboards."
    },
    {
      "name": "📝 Reset selected artboards",
      "identifier": "airsketch.reset-selected-artboards-identifier",
      "script": "sync-artboards.js",
      "shortcut": "cmd shift c",
      "handlers": {
        "run": "resetSelectedArtboards"
      },
      "description": "Reset text layers on selected artboards."
    },
    {
      "name": "⚙️ Settings",
      "identifier": "airsketch.settings-identifier",
      "script": "settings.js"
    }
  ],
  "menu": {
    "title": "AirSketch",
    "items": [
      "airsketch.settings-identifier",
      "-",
      "airsketch.sync-all-artboards-identifier",
      "airsketch.sync-selected-artboards-identifier",
      "-",
      "airsketch.reset-selected-artboards-identifier"
    ]
  },
  "version": "1.0.0",
  "identifier": "airsketch",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml",
  "author": "Tommy Mabrier"
}